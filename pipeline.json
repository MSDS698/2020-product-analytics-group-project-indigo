{
    "pipeline": {
        "name": "indigo_pipeline",
        "version": 1,
        "roleArn": "arn:aws:iam::374226464461:role/service-role/AWSCodePipelineServiceRole-us-west2-MSDS603-Week1",
        "artifactStore": {
            "type": "S3",
            "location": "codepipeline-msds603-week1"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name" : "GitHubSource",
                        "inputArtifacts": [],
                        "actionTypeId": {
                            "version": "1",
                            "owner": "ThirdParty",
                            "category": "Source",
                            "provider": "GitHub"
                        },
                        "outputArtifacts": [
                            {
                                "name": "SourceArtifact"
                            }
                        ],
                        "runOrder": 1,
                        "configuration": {
                            "Owner": "MSDS698",
                            "Repo": "2020-product-analytics-group-project-indigo",
                            "PollForSourceChanges": "true",
                            "Branch": "master",
                            "OAuthToken": "026b27f587bb399f3b0651c4750ddcf127b9583d"
                        }
                    }
                ]
            },

            {
              "name": "Deploy",
              "actions": [
                {
                  "inputArtifacts": [
                    {
                      "name": "SourceArtifact"
                    }
                  ],
                  "name": "CodeDeploy",
                  "actionTypeId": {
                    "category": "Deploy",
                    "owner": "AWS",
                    "version": "1",
                    "provider": "ElasticBeanstalk"
                  },
                  "outputArtifacts": [

                        ],
                  "configuration": {
                    "ApplicationName": "Indigo",
                    "EnvironmentName": "indigo-webserver"
                  },
                  "runOrder": 2
                }
              ]
            }
        ]
    }
}
